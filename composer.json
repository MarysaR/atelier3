{
  "name": "neycha/atelier3",
  "autoload": {
    "psr-4": {
      "Neycha\\Atelier3\\": "src/"
    }
  },
  "authors": [
    {
      "name": "neycha"
    }
  ],
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.76",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5"
  },
  "scripts": {
    "lint": "php-cs-fixer fix --dry-run",
    "analyse": "phpstan analyse",
